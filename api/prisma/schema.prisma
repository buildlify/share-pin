generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Session {
    id           String   @id @default(cuid())
    expires      DateTime
}

model User {
    id            String    @id @default(cuid())
    name          String?
    email         String?   @unique
}